{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/vue3/es2017",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"jest": true
	},
	"globals": {
		"global": "writable",
		"ve": "readonly"
	},
	"rules": {
		"jsdoc/check-alignment": "error"
	}
}
