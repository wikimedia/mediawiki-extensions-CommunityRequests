		},
		"ext.communityrequests.mint": {
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"codexComponents": [
				"CdxMessage",
				"CdxProgressBar",
				"CdxToggleSwitch"
			],
			"packageFiles": [
				"mint/init.js",
				"mint/TranslationBanner.vue",
