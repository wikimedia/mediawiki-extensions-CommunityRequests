{
	"name": "CommunityRequests",
	"author": [
		"[https://meta.wikimedia.org/wiki/Special:MyLanguage/Community_Tech WMF Community Tech team]"
	],
	"url": "https://www.mediawiki.org/wiki/Special:MyLanguage/Extension:CommunityRequests",
	"descriptionmsg": "communityrequests-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.45",
		"extensions": {
			"VisualEditor": "*"
		}
	},
	"suggests": {
		"extensions": {
			"Translate": "*"
		}
	},
	"TrackingCategories": [
		"communityrequests-error-category",
		"communityrequests-wish-category",
		"communityrequests-focus-area-category"
	],
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\CommunityRequests\\": "includes/"
	},
	"TestAutoloadNamespaces": {
		"MediaWiki\\Extension\\CommunityRequests\\Tests\\": "tests/phpunit/"
	},
	"TestAutoloadClasses": {
		"MediaWiki\\Extension\\CommunityRequests\\Maintenance\\NukeWishlist": "maintenance/nukeWishlist.php",
		"MediaWiki\\Extension\\CommunityRequests\\Maintenance\\DeleteOrphanedEntityRows": "maintenance/deleteOrphanedEntityRows.php",
		"MediaWiki\\Extension\\CommunityRequests\\Tests\\Integration\\WishlistTestTrait": "tests/phpunit/integration/WishlistTestTrait.php",
		"MediaWiki\\Extension\\CommunityRequests\\Tests\\Unit\\AbstractWishlistEntityTest": "tests/phpunit/unit/AbstractWishlistEntityTest.php"
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"MessagesDirs": {
		"CommunityRequests": [
			"i18n",
			"i18n/api"
		]
	},
	"ExtensionMessagesFiles": {
		"CommunityRequestsAlias": "CommunityRequests.i18n.alias.php"
	},
	"DatabaseVirtualDomains": [
		"virtual-communityrequests"
	],
	"Hooks": {
		"LoadExtensionSchemaUpdates": "schema",
		"LoginFormValidErrorMessages": "main",
		"BeforePageDisplay": "main",
		"ParserFirstCallInit": "main",
		"LinksUpdateComplete": "main",
		"PageDeleteComplete": "main",
		"RevisionDataUpdates": "main",
		"SkinTemplateNavigation::Universal": "main",
		"ParserAfterTidy": "main",
		"getUserPermissionsErrorsExpensive": "main",
		"BeforeDisplayNoArticleText": "main",
		"GetPreferences": "main",
		"ShowSearchHit": "search",
		"ChangeTagsListActive": "changeTag",
		"ListDefinedTags": "changeTag",
		"RecentChange_save": "changeTag"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\HookHandler\\CommunityRequestsHooks",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.WishStore",
				"CommunityRequests.FocusAreaStore",
				"CommunityRequests.VoteStore",
				"CommunityRequests.EntityFactory",
				"LinkRenderer",
				"PermissionManager",
				"SpecialPageFactory",
				"UserOptionsManager",
				"CommunityRequests.Logger",
				"MainConfig",
				"WikiPageFactory"
			]
		},
		"changeTag": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\HookHandler\\ChangeTagHooks",
			"services": [
				"CommunityRequests.WishlistConfig"
			]
		},
		"schema": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\HookHandler\\SchemaHooks",
			"factory": "MediaWiki\\Extension\\CommunityRequests\\HookHandler\\SchemaHooks::newFromGlobalState"
		},
		"search": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\HookHandler\\SearchHooks",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.WishStore",
				"CommunityRequests.FocusAreaStore",
				"CommunityRequests.Logger",
				"TitleFormatter"
			]
		}
	},
	"SpecialPages": {
		"WishlistIntake": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\Wish\\SpecialWishlistIntake",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.WishStore",
				"CommunityRequests.FocusAreaStore",
				"TitleParser",
				"UserFactory",
				"CommunityRequests.Logger"
			]
		},
		"EditFocusArea": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\FocusArea\\SpecialEditFocusArea",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.FocusAreaStore",
				"TitleParser",
				"CommunityRequests.Logger"
			]
		}
	},
	"APIListModules": {
		"communityrequests-wishes": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\Api\\ApiQueryWishes",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.Logger",
				"CommunityRequests.WishStore",
				"CommunityRequests.FocusAreaStore",
				"ExtensionRegistry"
			]
		}
	},
	"APIModules": {
		"wishedit": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\Api\\ApiWishEdit",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.Logger",
				"CommunityRequests.WishStore",
				"TitleParser",
				"ContentTransformer",
				"UserFactory"
			],
			"optional_services": [
				"Translate:TranslatablePageParser"
			]
		},
		"focusareaedit": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\Api\\ApiFocusAreaEdit",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.Logger",
				"CommunityRequests.FocusAreaStore",
				"TitleParser",
				"ContentTransformer"
			],
			"optional_services": [
				"Translate:TranslatablePageParser"
			]
		},
		"wishlistvote": {
			"class": "MediaWiki\\Extension\\CommunityRequests\\Api\\ApiWishlistVote",
			"services": [
				"CommunityRequests.WishlistConfig",
				"CommunityRequests.Logger",
				"WikiPageFactory",
				"CommunityRequests.VoteStore",
				"CommunityRequests.WishStore",
				"CommunityRequests.FocusAreaStore"
			]
		}
	},
	"AvailableRights": [
		"manage-wishlist",
		"manually-edit-wishlist"
	],
	"GroupPermissions": {
		"sysop": {
			"manage-wishlist": true,
			"manually-edit-wishlist": true
		},
		"community-wishlist-manager": {
			"manage-wishlist": true,
			"manually-edit-wishlist": true
		}
	},
	"DefaultUserOptions": {
		"usemachinetranslation": 0
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "CommunityRequsts/modules"
	},
	"ResourceModules": {
		"ext.communityrequests.styles": {
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"codexStyleOnly": true,
			"codexComponents": [
				"CdxButton",
				"CdxCard",
				"CdxIcon",
				"CdxInfoChip",
				"CdxMessage"
			],
			"styles": "common/styles.less"
		},
