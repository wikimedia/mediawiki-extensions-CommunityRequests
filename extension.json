				},
				"archived": {
					"id": 6,
					"label": "communityrequests-status-archived"
				}
			}
		}
	},
	"attributes": {
		"CodeMirror": {
			"TagModes": {
				"wish": "mediawiki"
			}
		}
	},
	"manifest_version": 2
}
